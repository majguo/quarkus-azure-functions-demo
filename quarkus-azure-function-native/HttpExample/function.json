{
  "bindings": [
    {
      "authLevel": "anonymous",
      "type": "httpTrigger",
      "direction": "in",
      "name": "req",
      "route" : "{*path}",
      "methods" : [ "GET", "HEAD", "POST", "PUT", "OPTIONS" ]
    },
    {
      "type": "http",
      "direction": "out",
      "name": "res"
    }
  ]
}
